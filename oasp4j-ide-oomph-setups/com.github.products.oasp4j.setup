        destinationFile="apache-maven-${maven.version}-bin.zip"
        priority="100"
        estimatedSize="8">
      <description>Downloads the maven binaries as a zip file</description>
    </setupTask>
    <setupTask
        xsi:type="com.github.maybeec.oomph.task:downloadTask"
        id="download.oasp"
        excludedTriggers="STARTUP"
        filter=""
        resourceLocation="https://github.com/oasp/oasp4j-ide/archive/master.zip"
        isURL="true"
        readTimeout="5000"
        destinationPath="${tmp.download.location}"
        destinationFile="oasp4j.zip"
        priority="100">
      <description>Downloads the main zip file for the OASP4J</description>
    </setupTask>
    <setupTask
        xsi:type="setup:CompoundTask"
        id="tasks.download.external.ressources.windows"
        filter="(os.name=Windows*)"
        name="windows">
      <setupTask
          xsi:type="com.github.maybeec.oomph.task:downloadTask"
          id="download.oasp.scripts"
          excludedTriggers="STARTUP"
          filter=""
          resourceLocation="http://repo1.maven.org/maven2/io/oasp/java/ide/oasp4j-ide-scripts/${oasp.scripts.version}/oasp4j-ide-scripts-${oasp.scripts.version}.zip"
          isURL="true"
          readTimeout="5000"
          destinationPath="${tmp.download.location}"
          destinationFile="oasp4j-ide-scripts-${oasp.scripts.version}.zip"
          priority="100">
        <description>Downloads the scripts for the OASP4j</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task:downloadTask"
          id="download.win.help"
          excludedTriggers="STARTUP"
          filter=""
          resourceLocation="https://raw.githubusercontent.com/oasp/oasp4j-ide/master/oasp4j-ide-oomph-setups/resources/help.bat"
          isURL="true"
          readTimeout="5000"
          destinationPath="${installation.root}"
          destinationFile="help.bat"
          priority="100">
        <description>Downloads a helper script</description>
      </setupTask>
      <setupTask
          xsi:type="setup:CompoundTask"
          id="tasks.download.external.ressources.windows.x64"
          filter="(osgi.arch=x86_64)"
          name="x64">
        <setupTask
            xsi:type="com.github.maybeec.oomph.task:downloadTask"
            id="download.tomcat"
            excludedTriggers="STARTUP"
            filter=""
            resourceLocation="http://archive.apache.org/dist/tomcat/tomcat-7/v${tomcat.version}/bin/apache-tomcat-${tomcat.version}-windows-x64.zip"
            isURL="true"
            readTimeout="5000"
            destinationPath="${tmp.download.location}"
            destinationFile="apache-tomcat-${tomcat.version}.zip"
            priority="100"
            estimatedSize="9">
          <description>Downloads the tomcat binaries as zip file</description>
        </setupTask>
        <setupTask
            xsi:type="com.github.maybeec.oomph.task:downloadTask"
            id="download.win.nodejs"
            excludedTriggers="STARTUP"
            filter=""
            resourceLocation="https://nodejs.org/dist/v${nodejs.version}/node-v${nodejs.version}-x64.msi"
            isURL="true"
            readTimeout="5000"
            destinationPath="${tmp.download.location}"
            destinationFile="node-v${nodejs.version}.msi"
            priority="100"
            estimatedSize="6">
          <description>Downloads a nodejs installer</description>
        </setupTask>
        <setupTask
            xsi:type="com.github.maybeec.oomph.task:downloadTask"
            id="download.win.python"
            excludedTriggers="STARTUP"
            filter=""
            resourceLocation="https://www.python.org/ftp/python/${python.version}/python-${python.version}.amd64.msi"
            isURL="true"
            readTimeout="5000"
            destinationPath="${tmp.download.location}"
            destinationFile="python-${python.version}.msi"
            priority="100"
            estimatedSize="225">
          <description>Downloads the python installable</description>
        </setupTask>
        <description>Download tasks for external ressources on windows x64</description>
      </setupTask>
      <setupTask
          xsi:type="setup:CompoundTask"
          id="tasks.download.external.ressources.windows.x32"
          filter="(osgi.arch=x86)"
          name="x32">
        <setupTask
            xsi:type="com.github.maybeec.oomph.task:downloadTask"
            id="download.tomcat"
            excludedTriggers="STARTUP"
            filter=""
            resourceLocation="http://archive.apache.org/dist/tomcat/tomcat-7/v${tomcat.version}/bin/apache-tomcat-${tomcat.version}-windows-x32.zip"
            isURL="true"
            readTimeout="5000"
            destinationPath="${tmp.download.location}"
            destinationFile="apache-tomcat-${tomcat.version}.zip"
            priority="100"
            estimatedSize="9">
          <description>Downloads the tomcat binaries as zip file</description>
        </setupTask>
        <setupTask
            xsi:type="com.github.maybeec.oomph.task:downloadTask"
            id="download.win.nodejs"
            excludedTriggers="STARTUP"
            filter=""
            resourceLocation="https://nodejs.org/dist/v${nodejs.version}/node-v${nodejs.version}-x86.msi"
            isURL="true"
            readTimeout="5000"
            destinationPath="${tmp.download.location}"
            destinationFile="node-v${nodejs.version}.msi"
            priority="100"
            estimatedSize="6">
          <description>Downloads a nodejs installer</description>
        </setupTask>
        <setupTask
            xsi:type="com.github.maybeec.oomph.task:downloadTask"
            id="download.win.python"
            excludedTriggers="STARTUP"
            filter=""
            resourceLocation="https://www.python.org/ftp/python/${python.version}/python-${python.version}.msi"
            isURL="true"
            readTimeout="5000"
            destinationPath="${tmp.download.location}"
            destinationFile="python-${python.version}.msi"
            priority="100"
            estimatedSize="225">
          <description>Downloads the python installable</description>
        </setupTask>
        <description></description>
      </setupTask>
      <description>Download tasks for external ressources on windows</description>
    </setupTask>
    <setupTask
        xsi:type="setup:CompoundTask"
        id="tasks.download.external.ressources.linux"
        filter="(osgi.os=linux)"
        name="linux">
      <setupTask
          xsi:type="com.github.maybeec.oomph.task:downloadTask"
          id="download.tomcat"
          excludedTriggers="STARTUP"
          filter=""
          resourceLocation="http://archive.apache.org/dist/tomcat/tomcat-7/v${tomcat.version}/bin/apache-tomcat-${tomcat.version}.zip"
          isURL="true"
          readTimeout="5000"
          destinationPath="${tmp.download.location}"
          destinationFile="apache-tomcat-${tomcat.version}.zip"
          priority="100"
          estimatedSize="9">
        <description>Downloads the tomcat binaries as zip file</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task:downloadTask"
          id="download.linux.python"
          excludedTriggers="STARTUP"
          filter=""
          resourceLocation="https://www.python.org/ftp/python/${python.version}/python-${python.version}.tgz"
          isURL="true"
          readTimeout="5000"
          destinationPath="${tmp.download.location}"
          destinationFile="python-${python.version}.tgz"
          priority="100"
          estimatedSize="15">
        <description>Downloads the python installable</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task:downloadTask"
          id="download.linux.oasp.scripts"
          excludedTriggers="STARTUP"
          resourceLocation="https://github.com/oasp/oasp4j-ide/raw/master/oasp4j-ide-oomph-setups/resources/oasp4j-ide-scripts-linux.zip"
          isURL="true"
          destinationPath="${tmp.download.location}"
          destinationFile="oasp4j-ide-scripts-${oasp.scripts.version}.zip"
          priority="100"/>
      <setupTask
          xsi:type="setup:CompoundTask"
          id="tasks.download.external.ressources.linux.x64"
          filter="(osgi.arch=x86_64)"
          name="x64">
        <setupTask
            xsi:type="com.github.maybeec.oomph.task:downloadTask"
            id="download.linux.nodejs"
            excludedTriggers="STARTUP"
            filter=""
            resourceLocation="https://nodejs.org/dist/v${nodejs.version}/node-v${nodejs.version}-linux-x64.tar.gz"
            isURL="true"
            readTimeout="5000"
            destinationPath="${tmp.download.location}"
            destinationFile="node-v${nodejs.version}.tgz"
            priority="100"
            estimatedSize="6">
          <description>Downloads a nodejs installer</description>
        </setupTask>
      </setupTask>
      <setupTask
          xsi:type="setup:CompoundTask"
          id="tasks.download.external.ressources.linux.x32"
          filter="(osgi.arch=x86)"
          name="x32">
        <setupTask
            xsi:type="com.github.maybeec.oomph.task:downloadTask"
            id="download.linux.nodejs"
            excludedTriggers="STARTUP"
            filter=""
            resourceLocation="https://nodejs.org/dist/v${nodejs.version}/node-v${nodejs.version}-linux-x86.tar.gz"
            isURL="true"
            readTimeout="5000"
            destinationPath="${tmp.download.location}"
            destinationFile="node-v${nodejs.version}.tgz"
            priority="100"
            estimatedSize="6">
          <description>Downloads a nodejs installer</description>
        </setupTask>
