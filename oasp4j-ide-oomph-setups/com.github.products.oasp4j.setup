  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      id="tasks.unzip.external.resources"
      name="unzip tasks of external resources">
    <setupTask
        xsi:type="com.github.maybeec.oomph.task_1:unzipTask"
        id="unzip.oasp"
        excludedTriggers="STARTUP"
        predecessor="download.oasp"
        zipFile="${tmp.download.location/}oasp4j.zip"
        destinationDir="${tmp.download.location}"
        priority="100">
      <description>Unzips the oasp into the supposed folder</description>
    </setupTask>
    <setupTask
        xsi:type="com.github.maybeec.oomph.task_1:unzipTask"
        id="unzip.ant"
        excludedTriggers="STARTUP"
        predecessor="download.ant"
        zipFile="${tmp.download.location/}apache-ant-${ant.version}-bin.zip"
        destinationDir="${software.location}"
        priority="100">
      <description>Unzips the ant binaries</description>
    </setupTask>
    <setupTask
        xsi:type="com.github.maybeec.oomph.task_1:unzipTask"
        id="unzip.elasticsearch"
        excludedTriggers="STARTUP"
        predecessor="download.elasticsearch"
        zipFile="${tmp.download.location/}elasticsearch-${elasticsearch.version}.zip"
        destinationDir="${software.location}"
        priority="100">
      <description>unzips the elasticsearch binaries</description>
    </setupTask>
    <setupTask
        xsi:type="com.github.maybeec.oomph.task_1:unzipTask"
        id="unzip.logstash"
        excludedTriggers="STARTUP"
        predecessor="download.logstash"
        zipFile="${tmp.download.location/}logstash-${logstash.version}.zip"
        destinationDir="${software.location}"
        priority="100">
      <description>Unzips the logstash binaries</description>
    </setupTask>
    <setupTask
        xsi:type="com.github.maybeec.oomph.task_1:unzipTask"
        id="unzip.maven"
        excludedTriggers="STARTUP"
        predecessor="download.maven"
        zipFile="${tmp.download.location/}apache-maven-${maven.version}-bin.zip"
        destinationDir="${software.location}"
        priority="100">
      <description>Unzips the maven binaries</description>
    </setupTask>
    <setupTask
        xsi:type="com.github.maybeec.oomph.task_1:unzipTask"
        id="unzip.tomcat"
        excludedTriggers="STARTUP"
        zipFile="${tmp.download.location/}apache-tomcat-${tomcat.version}.zip"
        destinationDir="${software.location}"
        priority="100">
      <description>Unzips the tomcat binaries</description>
    </setupTask>
    <setupTask
        xsi:type="com.github.maybeec.oomph.task_1:unzipTask"
        id="unzip.oasp.scripts"
        excludedTriggers="STARTUP"
        predecessor="compound.downloads"
        zipFile="${tmp.download.location/}oasp4j-ide-scripts-${oasp.scripts.version}.zip"
        destinationDir="${installation.root}"
        priority="100">
      <description></description>
    </setupTask>
    <setupTask
        xsi:type="setup:CompoundTask"
        id="tasks.unzip.external.resources.win"
        filter="(os.name=Windows*)"
        name="Windows"/>
    <setupTask
        xsi:type="setup:CompoundTask"
        id="tasks.download.external.ressources.linux"
        filter="(osgi.os=linux)"
        name="linux"/>
    <description></description>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      id="tasks.finalize.external.resources"
      name="finalize external resources">
    <setupTask
        xsi:type="setup:CompoundTask"
        id="tasks.finalize.external.resources.win"
        filter="(os.name=Windows*)"
        name="Windows">
      <setupTask
          xsi:type="fsmove:FSMoveTask"
          id="move.settings"
          predecessor="unzip.oasp.scripts"
          resource="${tmp.download.location/oasp4j-ide-master/oasp4j-ide-settings/src/main/settings}"
          destination="${settings.location/development/settings}"
          priority="100"/>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="mkdir.nodejs.folder"
          excludedTriggers="STARTUP"
          predecessor="download.win.nodejs"
          command="mkdir"
          priority="100">
        <argument>${software.location/nodejs}</argument>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="install.nodejs"
          excludedTriggers="STARTUP"
          command="msiexec"
          priority="100">
        <argument>/a</argument>
        <argument>&quot;${tmp.download.location/}node-v${nodejs.version}.msi&quot;</argument>
        <argument>/qn</argument>
        <argument>TARGETDIR=&quot;${software.location/nodejs/}&quot;</argument>
        <description>Extracts NodeJS</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="install.nodejs.reordering"
          excludedTriggers="STARTUP"
          command="xcopy"
          priority="100">
        <argument>&quot;${software.location/nodejs/nodejs}&quot;</argument>
        <argument>&quot;${software.location/nodejs}&quot;</argument>
        <argument>/s</argument>
        <argument>/e</argument>
        <argument>/i</argument>
        <argument>/h</argument>
        <argument>/y</argument>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="install.nodejs.cleanup"
          excludedTriggers="STARTUP"
          command="rmdir"
          priority="100">
        <argument>&quot;${software.location/nodejs/nodejs}&quot;</argument>
        <argument>/s</argument>
        <argument>/q</argument>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="install.nodejs.cleanup.msi"
          excludedTriggers="STARTUP"
          command="del"
          priority="100">
        <argument>&quot;${software.location/nodejs/}node-v${nodejs.version}.msi&quot;</argument>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="mkdir.python.folder"
          excludedTriggers="STARTUP"
          command="mkdir"
          priority="100">
        <argument>${software.location/python}</argument>
